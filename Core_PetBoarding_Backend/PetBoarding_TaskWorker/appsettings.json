{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Quartz": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=petboarding.postgres.database:5432;Database=petboarding;Username=postgres;Password=postgres"
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "PetBoarding TaskWorker",
    "quartz.scheduler.instanceId": "AUTO",
    "quartz.jobStore.type": "Quartz.Impl.AdoJobStore.JobStoreTX, Quartz",
    "quartz.jobStore.useProperties": "true",
    "quartz.jobStore.dataSource": "default",
    "quartz.jobStore.tablePrefix": "qrtz_",
    "quartz.jobStore.driverDelegateType": "Quartz.Impl.AdoJobStore.PostgreSQLDelegate, Quartz",
    "quartz.dataSource.default.provider": "Npgsql",
    "quartz.dataSource.default.connectionString": "Host=petboarding.postgres.database:5432;Database=petboarding;Username=postgres;Password=postgres",
    "quartz.serializer.type": "json",
    "quartz.jobStore.clustered": "true",
    "quartz.jobStore.clusterCheckinInterval": "20000"
  },
  "TaskWorker": {
    "ExpiredBasketCleanupIntervalMinutes": 10,
    "BasketExpirationMinutes": 30,
    "ExpiredReservationProcessingIntervalMinutes": 15
  }
}